    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "protractor": "^7.0.0",
    "ts-node": "9.1.1",
    "typescript": "4.3.5",
    "webdriver-manager": "^12.1.8"
  }
