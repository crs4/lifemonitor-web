  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.18",
    "@angular/cli": "^12.2.18",
    "@angular/compiler-cli": "12.2.17",
    "@angular/language-service": "12.2.17",
    "@types/datatables.net": "^1.10.21",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "2.0.8",
    "@types/jquery": "^3.5.9",
    "@types/node": "14.14.33",
    "admin-lte": "3.2.0",
